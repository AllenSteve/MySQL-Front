{$IMAGEBASE $400000}
{$DESCRIPTION 'The Delphi Inspiration -- DISQLite3 Personal'}
{$IMPLICITBUILD OFF}

requires
  rtl,
  designide,
  dbrtl;

contains
  DISQLite3Reg in '..\Source\DISQLite3Reg.pas',
  DISQLite3Database in '..\Source\DISQLite3Database.pas',
  DISQLite3DataSet in '..\Source\DISQLite3DataSet.pas',
  DISystemCompat in '..\Source\DISystemCompat.pas',
  DISQLite3Api;

end.
